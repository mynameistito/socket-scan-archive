{
  "name": "@kzg/socket-scan",
  "version": "1.0.0",
  "description": "Repository sync orchestrator for managing archived repos and Socket.dev scans",
  "private": true,
  "type": "module",
  "packageManager": "bun@1.3.4",
  "engines": {
    "bun": "1.3.4"
  },
  "author": {
    "name": "KillzoneGaming",
    "email": "tito@kzg.gg",
    "url": "https://kzg.gg"
  },
  "license": "UNLICENSED",
  "scripts": {
    "start": "bun scripts/cleanup.ts && bun scripts/repo-sync-orchestrator.ts",
    "start:dry": "bun scripts/cleanup.ts && bun scripts/repo-sync-orchestrator.ts --dry-run",
    "check-config": "bun scripts/repo-sync-orchestrator.ts --check-config",
    "fix": "ultracite fix",
    "lint": "ultracite check",
    "ci": "biome ci",
    "tsc": "tsc --noEmit"
  },
  "dependencies": {
    "@octokit/rest": "^21.0.0",
    "@socketsecurity/cli": "^2.1.0",
    "axios": "^1.6.8",
    "dotenv": "^16.4.5"
  },
  "devDependencies": {
    "@biomejs/biome": "2.3.11",
    "@types/bun": "^1.3.5",
    "typescript": "^5.9.3",
    "ultracite": "7.0.10"
  }
}
