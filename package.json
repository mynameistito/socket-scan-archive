{
  "name": "@kzg/socket-scan",
  "version": "1.0.0",
  "description": "Repository sync orchestrator for managing archived repos and Socket.dev scans",
  "private": true,
  "type": "module",
  "packageManager": "bun@1.3.5",
  "engines": {
    "bun": "1.3.5"
  },
  "author": {
    "name": "mynameistito",
    "email": "github@mynameistito.com",
    "url": "https://mynameistito.com"
  },
  "license": "MIT",
  "scripts": {
    "start": "bun scripts/cleanup.ts && bun scripts/repo-sync-orchestrator.ts",
    "start:dry": "bun scripts/cleanup.ts && bun scripts/repo-sync-orchestrator.ts --dry-run",
    "check-config": "bun scripts/repo-sync-orchestrator.ts --check-config",
    "fix": "ultracite fix",
    "lint": "ultracite check",
    "ci": "biome ci",
    "tsc": "tsc --noEmit"
  },
  "dependencies": {
    "@octokit/rest": "^22.0.1",
    "@socketsecurity/cli": "^1.1.55",
    "axios": "^1.13.2",
    "dotenv": "^17.2.3"
  },
  "devDependencies": {
    "@biomejs/biome": "2.3.11",
    "@types/bun": "^1.3.5",
    "typescript": "^5.9.3",
    "ultracite": "7.0.11"
  }
}
